    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "3.10.2",
    "eslint-plugin-unicorn": "29.0.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
